<%= form_for(@publish) do |f| %>


<div class="form-group">
	<%= f.label :description %>
	<%= f.text_field :description, type: 'text', :class =>"form-control" %>
</div>


<div class="form-group">
	<%= f.label :country %>
	<%= f.text_field :country, type: 'text', :class =>"form-control" %>
</div>

</br>
<div class="form-group">
	<%= f.label :image %>
	<%= f.file_field :image %>
</div>

</br>


</br>
<%= f.button :submit , class: "btn btn-dark" %>
</br>

</div>

<% end %>

